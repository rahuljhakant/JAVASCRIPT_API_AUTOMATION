{
  "source": {
    "include": [
      ".",
      "README.md"
    ],
    "includePattern": "\\.(js|mjs|ts)$",
    "exclude": [
      "node_modules",
      "dist",
      "coverage",
      "allure-results",
      "allure-report",
      "screenshots",
      "test-results",
      "educational_hacking_tutorials"
    ]
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}

